{
  "name": "unicodeforyou",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "clipboard": "^1.5.16",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.0",
    "postcss-loader": "^1.2.2",
    "react-lite": "^0.15.31",
    "style-loader": "^0.13.1",
    "sync-request": "^4.0.1",
    "tachyons": "^4.6.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^2.0.2"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack --env production --progress",
    "deploy": "npm run jekyll && npm run build && surge dist --domain=https://unicodeforyou.surge.sh",
    "jekyll": "jekyll build",
    "scrape": "babel-node ./utils/scraper.js --presets es2015"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 9"
            ]
          }
        }
      ],
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
